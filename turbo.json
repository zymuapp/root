{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [
				".next/**",
				"!.next/cache/**",
				"dist/**/*.d.ts",
				"dist/**/*.mjs",
				"dist/**/*.js",
				"dist/**/*.css",
				"index.js",
				"ssg.js",
				"loader.js"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {},
		"lint": {
			"outputs": [],
			"inputs": [
				"src/**/*.tsx",
				"src/**/*.ts",
				"src/**/*.js",
				"src/**/*.jsx",
				"biome.json"
			]
		},
		"format": {
			"outputs": [],
			"inputs": [
				"src/**/*.tsx",
				"src/**/*.ts",
				"src/**/*.js",
				"src/**/*.jsx",
				"biome.json"
			]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		}
	}
}
